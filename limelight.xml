<?xml version="1.0" encoding="UTF-8" ?>
<html xmlns='http://www.w3.org/1999/xhtml' xmlns:b='http://www.google.com/2005/gml/b' xmlns:data='http://www.google.com/2005/gml/data' xmlns:expr='http://www.google.com/2005/gml/expr'>
  <head>
    <meta content='IE=EmulateIE7' http-equiv='X-UA-Compatible'/>
    <!-- If you delete this meta tag World War Z will become a reality -->
    <meta content='width=device-width, initial-scale=1.0' name='viewport'/>
    <link charset='utf-8' href='http://cdn.foundation5.zurb.com/foundation.css' media='screen' rel='stylesheet' title='no title'/>
    <b:include data='blog' name='all-head-content'/>
    <title><data:blog.pageTitle/></title>
    <b:skin>
     <![CDATA[
      /*body {
       font: $(body.font);
       color: $(body.text.color);
       background: $(body.background);
       padding: 0 $(content.shadow.spread) $(content.shadow.spread) $(content.shadow.spread);
       $(body.background.override) margin: 0;
       padding: 0;
      }*/
      /* Custom styles
       * -------------
       */
      /* Labels
       * Removes zurb default lable styles from label widget
       */
      .widget.Label {
        text-align: left;
        color: #222;
        background: transparent;
      }
      /* Navigation */
      #header ul {
          list-style: none;
          margin: 0;
          left: 0;
      }
      #header ul:after {
          content: " ";
          display: table;
      }
      #header ul li {
          margin: 0 -2px;
          display: inline-block;
      }
      #header li a {
          padding: .6em 1em;
      }
      /* hide wiget title */
      #header h2 {
          display: none;
      }

      /* Featured element */
      #crosscol-overflow h2 {
          margin-bottom: 8px;
          padding-top: 30px;
          padding-bottom: 10px;
          font-size: 11px;
          letter-spacing: .1em;
          text-transform: uppercase;
          border-bottom: 1px solid #ccc;
      }
      #crosscol-overflow h3 {
          font-size: 36px;
          font-weight: bold;
      }
     ]]>
    </b:skin>
  </head>
  <body>

    <!-- Blogger Header section -->
    <div class='row'>
      <div class='large-12 columns'>
        <b:section class='header' id='header' maxwidgets='2' showaddelement='no'>
          <b:widget id='PageList1' locked='false' title='Pages' type='PageList'/>
          <b:widget id='Header1' locked='false' title='LORENZO&apos;SMUSIC (Header)' type='Header'/>
        </b:section>
      </div>
    </div>

    <!-- Blogger Crosscol section -->
    <div class='row'>
      <div class='large-12 columns'>
        <b:section class='tabs' id='crosscol' maxwidgets='1' showaddelement='yes'/>
        <b:section class='tabs' id='crosscol-overflow' showaddelement='no'>
          <b:widget id='HTML3' locked='false' title='Featured' type='HTML'/>
        </b:section>
      </div>
    </div>

    <!-- main row -->
    <div class='row'>

      <!-- Blogger main section -->
      <div class='large-9 columns' role='content'>
        <b:section class='main' id='main' showaddelement='yes'>
          <!-- blogger posts -->
          <b:widget id='Blog1' locked='true' title='Blog Posts' type='Blog'/>
        </b:section>
      </div>
      <!-- /end Blogger main section -->

      <!-- Blogger sidebar section -->
      <div class='large-3 columns'>
        <b:section class='sidebar' id='sidebar' preferred='yes'>
          <!-- Sidebar gadgets and widgets code -->
          <b:widget id='Label1' locked='false' title='Labels' type='Label'/>
        </b:section>
      </div>
      <!-- /end Blogger sidebar section -->

    </div><!-- /end main row -->

    <!-- Blogger Footer section -->
    <div class='row'>
      <div class='large-12 columns'>
        <b:section class='foot' id='footer' showaddelement='yes'/>
      </div>
    </div>
    <!-- /end Blogger Footer -->

    <!-- js -->
    <script src='https://code.jquery.com/jquery-2.1.4.min.js'/>
    <script src='http://cdn.foundation5.zurb.com/foundation.js'/>
    <script type='text/javascript'>
      $(document).foundation();
    </script>

  </body>
</html>
